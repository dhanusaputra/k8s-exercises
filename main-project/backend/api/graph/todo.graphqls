# GraphQL schema example
#
# https://gqlgen.com/getting-started/

type Todo {
  id: ID!
  text: String
  done: Boolean
}

type Query {
  todos: [Todo!]!
}

input TodoInput {
  text: String
  done: Boolean
}

type Mutation {
  createTodo(input: TodoInput!): Todo
  updateTodo(id: ID!, input: TodoInput!): Todo
}
